{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/nft-metadata/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/nft-metadata",
        "index": "apps/nft-metadata/src/index.html",
        "baseHref": "/",
        "main": "apps/nft-metadata/src/main.tsx",
        "polyfills": "apps/nft-metadata/src/polyfills.ts",
        "tsConfig": "apps/nft-metadata/tsconfig.app.json",
        "assets": [
          "apps/nft-metadata/src/favicon.ico",
          "apps/nft-metadata/src/assets"
        ],
        "styles": ["apps/nft-metadata/src/styles.css"],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/nft-metadata/src/environments/environment.ts",
              "with": "apps/nft-metadata/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        }
      }
    },
    "tailwindcss": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "tailwindcss -c tailwind.config.js -i libs/design-system/styles/main.css -o apps/nft-metadata/src/app/styles/tailwind.css --watch"
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "nft-metadata:build",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "nft-metadata:build:production",
          "hmr": false
        },
        "development": {
          "buildTarget": "nft-metadata:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/nft-metadata/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/nft-metadata"],
      "options": {
        "jestConfig": "apps/nft-metadata/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
